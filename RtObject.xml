  <group name="Bools" description="A few booleans" hidden="false" >
    <item name="boolA1" displayName="Boolean #1" description="This is a boolean value defaulting to False." defaultValue="False" type="Bool" hidden="false" />
    <item name="boolA2" displayName="Boolean #2" description="This is a boolean value defaulting to True." defaultValue="True" type="Bool" hidden="false" />
  </group>
  <group name="Ints" description="Some integers" hidden="false" >
    <item name="intA1" displayName="Int32 #1" description="Int32 with a range of 10-20" defaultValue="11" type="Int32" hidden="false" min="10" max="20" />
