<?xml version="1.0" encoding="utf-8"?>
<RtObject>
  <group name="Strings" expanded="true" description="A few strings" hidden="false">
    <item name="stringA1" type="String" description="String with a size range of 10-20" defaultValue="defaultValue" displayName="String #1" min="10" max="20"/>
    <item name="stringA2" type="String" description="String with min size of 10." defaultValue="default" displayName="String #2" min="10" />
    <item name="stringA3" type="String" description="String with max size of 10." defaultValue="default" displayName="String #3" max="10" /> />
  </group>
  
  <group name="Bools" expanded="false" description="A few booleans">
    <item name="boolA1" displayName="Boolean #1" type="Bool" description="This is a boolean value defaulting to False." defaultValue="False" />
    <item name="boolA2" displayName="Boolean #2" type="Bool" description="This is a boolean value defaulting to True." defaultValue="True" />
  </group>
  <group name="Ints" expanded="false" description="Some integers">
    <item name="intA1" type="Int32" description="Int32 with a range of 10-20" defaultValue="1" dispblayName="Int32 #1" min="10" max="20"/>
    <item name="intA2" type="Int32" description="Int32 with min of 10." defaultValue="2" displayName="Int32 #2" min="10" />
    <item name="intA3" type="Int32" description="Int32 with max of 10." defaultValue="3" displayName="Int32 #3" max="10" /> />
  </group>
  <group name="Enums" description="Enumerations are fun!">
    <item name="enumD1" type="EnumA" description="EnumA description" defaultValue="enumKeyA" displayName="Enumeration A"/>
    <item name="enumD2" type="EnumB" description="EnumB description" defaultValue="enumKeyZ" displayName="Enumeration B" />
    <item name="enumD3" type="EnumC" description="EnumC description" defaultValue="" displayName="Enumeration C" /> />
  </group>
  <group name="Grouped" description="Array of itemB objects with min number of 3." arrayValueName="itemB" min="3" key="-&gt;description name//">
    <item displayName="Name" name="name" type="string" description="Name, no spaces" />
    <item displayName="Description" name="description" type="string" description="description" />
    
    <group name="Nested Tab" description="this is another group">
      <item name="enumD1" type="EnumA" description="EnumA description" defaultValue="enumKeyA" displayName="Enumeration A"/>
      <item name="enumD2" type="EnumB" description="EnumB description" defaultValue="enumKeyZ" displayName="Enumeration B" />
    </group>
  </group>

  <group name="Simple Array" description="Array of itemAS objects with a group.  Min 2" arrayValueName="itemAS" min="2">
    <group name="Basic" description="this is the basic stuff">
      <item displayName="Name for A" name="nameA" type="string" description="Name, no spaces" />
      <item displayName="Description for A" name="descriptionA" type="string" description="description" />
    </group>
    <group name="Other" description="this is other stuff">
      <item displayName="String" name="stringInA" type="string" description="Name, no spaces" />
      <item displayName="Int" name="intInA" type="Int32" description="an integer" />
    </group>
  </group>

  <group name="Nested Array" description="Array of itemA objects nested with ItemBs with min number of 0." arrayValueName="itemA" min="0">
    <group name="A">
      <item displayName="Name for A" name="nameA" type="string" description="Name, no spaces" />
      <item displayName="Description for A" name="descriptionA" type="string" description="description" />
    </group>
    <group name="ItemB Array" description="Array of itemB objects with min number of 3." arrayValueName="itemAB" min="0" key="nameB nameB2 nameB">
      <item displayName="Name for B" name="nameB" type="string" description="Name, no spaces" />
      <item displayName="Name part 2 for B" name="nameB2" type="string" description="Name, no spaces" />
      <item displayName="Description for B" name="descriptionB" type="string" description="description" />
      <group name="ItemC Array" description="Array of itemC objects." arrayValueName="itemAB" min="0" key="nameC">
        <item displayName="Name for B" name="nameC" type="string" description="Name, no spaces" />
        <item displayName="Name part 2 for B" name="descC" type="Int32" description="Name, no spaces" />
      </group>
    </group>
  </group>

  <enum name="EnumA">
    <value key="enumKeyA" value="Enum A" />
    <value key="enumKeyB" value="Enum B" />
    <value key="enumKeyC" value="Enum C" />
  </enum>
  <enum name="EnumB">
    <value key="enumKeyX" value="Enum X" />
    <value key="enumKeyY" value="Enum Y" />
    <value key="enumKeyZ" value="Enum Z" />
  </enum>
  <enum name="EnumC">
    <value key="enumKeyQ" value="Enum Q" />
    <value key="enumKeyP" value="Enum P" />
  </enum>
</RtObject>