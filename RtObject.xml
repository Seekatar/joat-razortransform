    <item readOnly="true" name="RofolderS1" displayName="Folder name" description="Folder Description" defaultValue="c:\" type="Folder" hidden="false" />
    <item readOnly="true" name="RopasswordS1" displayName="Password" description="enter a password" defaultValue="" type="Password" hidden="false" />
    <item readOnly="true" name="RoguidS1" displayName="Guid" description="enter a password" defaultValue="" type="Guid" hidden="false" />
  </group>

