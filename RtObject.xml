<?xml version="1.0" encoding="utf-8"?>
<RtObject>
  <group name="Hello" description="Welcome to Artie" hidden="false">
    <item name="w1" displayName="Hi" description="Welcome String1" defaultValue="This is a sample configuration" type="String" readOnly="true" />
    <item name="w2" displayName="Help" description="Welcome String2" defaultValue="Click the Gear for links to the documentation" type="String" readOnly="true" />
    <item name="w6" displayName="More Help" description="Welcome String2" defaultValue="Hover over names and values for help." type="String" readOnly="true" />
    <item name="w3" displayName="Go!" description="Welcome String3" defaultValue="Click the play button to transform these values into output" type="String" readOnly="true" />
    <item name="w4" displayName="Where?" description="Welcome String4" defaultValue="LocalAppData\RazorTransform has these sample files" type="String" readOnly="true" />
    <item name="w5" displayName="Output?" description="Welcome String5" defaultValue="After running output is in the folder in the title bar" type="String" readOnly="true" />
  </group>
  <group name="Sampler" description="One of each" hidden="false">
    <item name="s1" displayName="Sampler" description="Sample string" defaultValue="This tab has one of each simple type." type="String" readOnly="true" />
    <item name="colorS1" displayName="Color" description="pick a color" defaultValue="Blue" type="ColorType" />
    <item name="stringS1" displayName="Name" description="String with a size range of 10-20" defaultValue="Fred Flintstone" type="String" hidden="false" min="10" max="20" />
    <item name="boolS1" displayName="Check" description="This is a boolean value defaulting to False." defaultValue="False" type="Bool" hidden="false" />
    <item name="intS1" displayName="Limit" description="Int32 with a range of 10-20" defaultValue="11" type="Int32" hidden="false" min="10" max="20" />
    <item name="enumS2" displayName="Animal" description="animals" defaultValue="cow" type="Animals" hidden="false" />
    <item name="uncS1" displayName="UNC Folder name" description="Unc Description" defaultValue="c:\" type="UncPath" hidden="false" />
    <item name="folderS1" displayName="Folder name" description="Folder Description" defaultValue="c:\" type="Folder" hidden="false" />
    <item name="passwordS1" displayName="Password" description="enter a password" defaultValue="" type="Password" hidden="false" />
    <item name="guidS1" displayName="Guid" description="Click to create a Guid" defaultValue="" type="Guid" hidden="false" />
  </group>

  <group name="Strings" description="A few strings" hidden="false">
    <item name="s2" displayName="Strings" description="Sample string" defaultValue="This tab has strings with differnt size ranges." type="String" readOnly="true" />
    <item name="stringA1" displayName="String Range" description="String with a size range of 10-20" defaultValue="defaultValue" type="String" hidden="false" min="10" max="20" />
    <item name="stringA2" displayName="String Min" description="String with min size of 10." defaultValue="another default value" type="String" hidden="false" min="10" />
    <item name="stringA3" displayName="String Max" description="String with max size of 10." defaultValue="test" type="String" hidden="false" max="10" />
    <item name="version4" displayName="Version4" description="RegEx" defaultValue="1.0.0.0" type="String" regex="version4" hidden="false" />
    <item name="version2" displayName="Version2" description="RegEx" defaultValue="1.0" type="String" regex="version2" hidden="false" />
  </group>

  <group name="Ints" description="Some integers" hidden="false" >
    <item name="s4" displayName="Ints" description="Sample string" defaultValue="This tab has ints with different limits." type="String" readOnly="true" />
    <item name="intA1" displayName="Int32 #1" description="Int32 with a range of 10-20" defaultValue="11" type="Int32" hidden="false" min="10" max="20" />
    <item name="intA2" displayName="Int32 #2" description="Int32 with min of 10." defaultValue="12" type="Int32" hidden="false" min="10" />
    <item name="intA3" displayName="Int32 #3" description="Int32 with max of 10." defaultValue="13" type="Int32" hidden="false" max="10" />
  </group>
  
  <group name="Grouped" description="Array of itemB objects with min number of 3." hidden="false" arrayValueName="itemB" min="3" key="-&gt;description name//" >
    <item name="name" displayName="Name" description="Name, no spaces" regex="No spaces" defaultValue="" type="string" hidden="false" />
    <item name="description" displayName="Description" description="description" defaultValue="" type="string" hidden="false" />

    <group name="Nested Tab" description="this is another group" hidden="false" >
      <item name="enumD1" displayName="Enumeration A" description="EnumA description" defaultValue="enumKeyA" type="EnumA" hidden="false" />
      <item name="enumD2" displayName="Enumeration B" description="EnumB description" defaultValue="enumKeyZ" type="EnumB" hidden="false" />
    </group>
  </group>

  <group name="Simple Array" description="Array of itemAS objects with a group.  Min 2, unique name." hidden="false" arrayValueName="itemAS" min="2" unique="true">
    <group name="Basic" description="this is the basic stuff" hidden="false" >
      <item name="nameA" displayName="Name for A" description="Name, no spaces" defaultValue="" type="string" hidden="false" />
      <item name="descriptionA" displayName="Description for A" description="description" defaultValue="" type="string" hidden="false" />
    </group>
    <group name="Other" description="this is other stuff" hidden="false" >
      <item name="stringInA" displayName="String" description="Name, no spaces" defaultValue="" type="string" hidden="false" />
      <item name="intInA" displayName="Int" description="an integer" defaultValue="" type="Int32" hidden="false" />
    </group>
  </group>

  <group name="Nested Array" description="Array of itemA objects nested with ItemBs with min number of 0." hidden="false" arrayValueName="itemA" key="&gt;nameA" >
    <group name="A" description="" hidden="false" >
      <item name="nameA" displayName="Name for A" description="Name, no spaces" defaultValue="" type="string" hidden="false" />
      <item name="descriptionA" displayName="Description for A" description="description" defaultValue="" type="string" hidden="false" />
    </group>
    <group name="ItemB Array" description="Array of itemB objects with min number of 3." hidden="false" arrayValueName="itemAB" key="Key = nameB nameB2" >
      <item name="nameB" displayName="Name for B" description="Name, no spaces" defaultValue="" type="string" hidden="false" />
      <item name="nameB2" displayName="Name part 2 for B" description="Name, no spaces" defaultValue="" type="string" hidden="false" />
      <item name="descriptionB" displayName="Description for B" description="description" defaultValue="" type="string" hidden="false" />
      <group name="ItemC Array" description="Array of itemC objects." hidden="false" arrayValueName="itemAB" key="nameC" >
        <item name="nameC" displayName="Name for B" description="Name, no spaces" defaultValue="" type="string" hidden="false" />
        <item name="descC" displayName="Name part 2 for B" description="Name, no spaces" defaultValue="" type="Int32" hidden="false" />
      </group>
    </group>
  </group>

  <group name="RO" description="One of each read only " hidden="false">
    <item readOnly="true" name="RostringS1" displayName="String #1" description="String with a size range of 10-20" defaultValue="defaultValue" type="String" hidden="false" min="10" max="20" />
    <item readOnly="true" name="RoboolS1" displayName="Boolean #1" description="This is a boolean value defaulting to False." defaultValue="False" type="Bool" hidden="false" />
    <item readOnly="true" name="RointS1" displayName="Int32 #1" description="Int32 with a range of 10-20" defaultValue="11" type="Int32" hidden="false" min="10" max="20" />
    <item readOnly="true" name="RoenumS2" displayName="Enumeration B" description="EnumB description" defaultValue="enumKeyZ" type="EnumB" hidden="false" />
    <item readOnly="true" name="RouncS1" displayName="UNC Folder name" description="Unc Description" defaultValue="c:\" type="UncPath" hidden="false" />
    <item readOnly="true" name="RofolderS1" displayName="Folder name" description="Folder Description" defaultValue="c:\" type="Folder" hidden="false" />
    <item readOnly="true" name="RopasswordS1" displayName="Password" description="enter a password" defaultValue="" type="Password" hidden="false" />
    <item readOnly="true" name="RoguidS1" displayName="Guid" description="enter a password" defaultValue="" type="Guid" hidden="false" />
  </group>

  <custom name="ColorType" classname="RazorTransform.Custom.ColorType,RazorTransform"/>

  <enum name="EnumA">
    <value key="enumKeyA" value="Enum A" />
    <value key="enumKeyB" value="Enum B" />
    <value key="enumKeyC" value="Enum C" />
  </enum>
  <enum name="EnumB">
    <value key="enumKeyX" value="Enum X" />
    <value key="enumKeyY" value="Enum Y" />
    <value key="enumKeyZ" value="Enum Z" />
  </enum>
  <enum name="EnumC">
    <value key="enumKeyQ" value="Enum Q" />
    <value key="enumKeyP" value="Enum P" />
  </enum>
  <enum name="Animals">
    <value key="cow" value="Cow"/>
    <value key="pig" value="Pig"/>
    <value key="horse" value="Horse"/>
    <value key="chicken" value="Chicken"/>
  </enum>
  <regex name="version4" value="^\d+\.\d+\.\d+\.\d+$" />
  <regex name="version2" value="^\d+\.\d+$" />
  <regex name="No spaces" value="^[^\s]+$" />

</RtObject>